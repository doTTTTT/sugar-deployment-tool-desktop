<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints percentWidth="30.0" />
        <ColumnConstraints fillWidth="true" percentWidth="70.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints fillHeight="true" />
    </rowConstraints>
    <children>
        <GridPane maxWidth="Infinity" GridPane.columnIndex="0" GridPane.rowIndex="0">
            <columnConstraints>
                <ColumnConstraints percentWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            </rowConstraints>
            <children>
                <Button fx:id="devices" maxWidth="Infinity" mnemonicParsing="false" text="Devices" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <Button fx:id="inventory" maxWidth="Infinity" mnemonicParsing="false" text="Inventory" GridPane.columnIndex="0" GridPane.rowIndex="1" disable="true"/>
                <Button fx:id="appManager" maxWidth="Infinity" mnemonicParsing="false" text="Application Manager" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                <Button fx:id="filePusher" maxWidth="Infinity" mnemonicParsing="false" text="File Pusher" GridPane.columnIndex="0" GridPane.rowIndex="3" disable="true" />
                <Button fx:id="fileDeleter" maxWidth="Infinity" mnemonicParsing="false" text="File Deleter" GridPane.columnIndex="0" GridPane.rowIndex="4" disable="true" />
                <Button fx:id="createInstruction" maxWidth="Infinity" mnemonicParsing="false" text="Create Instruction" GridPane.columnIndex="0" GridPane.rowIndex="5" disable="false" />
                <Button fx:id="loadInstruction" maxWidth="Infinity" mnemonicParsing="false" text="Load Instruction" GridPane.columnIndex="0" GridPane.rowIndex="6" disable="false" />
                <Button fx:id="settings" maxWidth="Infinity" mnemonicParsing="false" text="Settings" GridPane.columnIndex="0" GridPane.rowIndex="7" disable="true" />
            </children>
        </GridPane>
        <BorderPane fx:id="container" GridPane.columnIndex="1" GridPane.columnSpan="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.vgrow="ALWAYS">
        </BorderPane>
    </children>
</GridPane>
